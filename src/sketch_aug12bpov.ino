void setup()
 { Serial.begin(9600);

 for( int i = 2; i<10 ;i++ )    // setting the ports of the leds to OUTPUT
 { pinMode(i, OUTPUT);
 }

 letterSpace = 2;     // defining the space between the letters


 dotTime =1;        // defining the time dots appear
}


 void printLetter(int letter[])
 {
 int y;
          // printing the first y row of the letter
 for (y=0; y<8; y++)
 {
 digitalWrite(y+2, letter[y]);
 }
 delay(dotTime);
          // printing the second y row of the letter
 for (y=0; y<8; y++)
 {
 digitalWrite(y+2, letter[y+8]);
 }
 delay(dotTime);
          // printing the third y row of the letter
 for (y=0; y<8; y++)
 {
 digitalWrite(y+2, letter[y+16]);
 }
 delay(dotTime);
          // printing the fourth y row of the letter
 for(y = 0; y<8; y++) {
 digitalWrite(y+2, letter[y+24]);
 }
 delay(dotTime);

 for(y = 0; y<8; y++) {     // printing the third y row of the letter
 digitalWrite(y+2, letter[y+32]);
 }
 delay(dotTime);

 for (y=0; y<8; y++)      // printing the sspace between the letters
 {
 digitalWrite(y+2, 0);
 }
 delay(letterSpace);
 }
 



